{% extends "base.html.twig" %}

{% block title %}
Artykuły
{% endblock %}

{% block body %}
<h1>Witam Cię w moich artykułach!</h1>
<div>
    {% for article in articles %}
    <a href="{{ article.link }}" target="_blank">
        <h2>{{ article.title }}</h2>
        <p>{{ article.content }}</p>
    </a>
    {% endfor %}
</div>
{% endblock%}